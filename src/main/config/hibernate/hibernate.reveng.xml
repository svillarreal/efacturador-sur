<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<!-- SELECT '<table-filter match-name="' || U.TABLE_NAME || '" package="com.topgroup.blankapp.base.model" />' FROM USER_TABLES 
	U ORDER BY TABLE_NAME ASC -->
<hibernate-reverse-engineering>

	<schema-selection match-schema="LOGIOS_TEST" />

	<type-mapping>
		<sql-type jdbc-type="NUMERIC" precision="10" hibernate-type="integer" />
		<sql-type jdbc-type="NUMERIC" precision="5" hibernate-type="integer" />
		<sql-type jdbc-type="NUMERIC" precision="1" hibernate-type="java.lang.Boolean" />
		<sql-type jdbc-type="CHAR" precision="1" hibernate-type="string" />
	</type-mapping>

	<!-- <table-filter match-name="TABLE_NAME" package="com.topgroup.logios.entidades" /> -->

</hibernate-reverse-engineering>