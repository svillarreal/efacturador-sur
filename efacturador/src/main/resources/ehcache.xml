<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="false"
	monitoring="autodetect" dynamicConfig="true">

	<!-- <diskStore path="user.dir/efacturador-cache"/> -->

	<defaultCache eternal="false" overflowToDisk="false"
		maxElementsInMemory="1000" timeToIdleSeconds="300" timeToLiveSeconds="600"
		statistics="false" />

	<cache name="org.hibernate.cache.StandardQueryCache"
		maxElementsInMemory="10000" eternal="false" timeToIdleSeconds="180"
		timeToLiveSeconds="150" overflowToDisk="false" />


	<!-- <cache name="productos" maxElementsInMemory="1000" eternal="false" 
		overflowToDisk="false" timeToLiveSeconds="150" copyOnRead="true"/> -->


</ehcache>